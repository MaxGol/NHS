{
  "name": "national_hope_service_bots",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "standard --fix --verbose | snazzy",
    "build": "npm run webpack:client && serverless package",
    "ngrok": "if [ -z \"${LOCAL}\" ]; then ngrok start --all --config ./ngrok.yml; fi",
    "server": "AWS_XRAY_CONTEXT_MISSING=LOG_ERROR serverless offline start --stage development --location build/server/service",
    "start": "rm -rf build/ && npm run db:install && concurrently \"npm run server\" \"npm run ngrok\" \"npm run webpack\" \"npm run watch\"",
    "webpack": "concurrently \"webpack-dev-server --config webpack/cms.js\"",
    "webpack:client": "concurrently \"webpack --config webpack/cms.js\"",
    "watch": "onchange \"*.js\" \"webpack/*/*.js\" \"src/**/*.js\" \"src/**/*.jsx\" -- npm run test",
    "deploy:staging": "export NODE_ENV=production STAGE=staging && npm run webpack:client && serverless deploy --stage $STAGE --verbose",
    "deploy:production": "export NODE_ENV=production STAGE=production && npm run webpack:client && serverless deploy --stage $STAGE --verbose",
    "db:install": "sls dynamodb install --stage development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
